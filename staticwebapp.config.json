{
    "routes": [
      {
        "route": "/*",
        "allowedRoles": ["admin"]
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/aad"
      }
    }
  }